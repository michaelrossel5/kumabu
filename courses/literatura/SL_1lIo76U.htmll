  <script>
    // Función para obtener el historial desde el almacenamiento local
    function getHistory() {
      const history = localStorage.getItem("history");
      return history ? JSON.parse(history) : [];
    }

    // Función para guardar el historial en el almacenamiento local
    function saveHistory(history) {
      localStorage.setItem("history", JSON.stringify(history));
    }

    // Función para agregar un ID al historial (sin URL completa)
    function addPageToHistory(id, title) {
      const history = getHistory();
      const timestamp = new Date().toLocaleString();
      const newPage = { id, title, timestamp };
      history.push(newPage);
      saveHistory(history);
    }

    // Agregar la página actual al historial
    window.onload = function() {
      const currentId = document.getElementById('page-id').textContent;  // Usando el ID como referencia
      const currentTitle = document.title;  // Título de la página
      addPageToHistory(currentId, currentTitle);  // Guardar el ID y el título en el historial
      console.log("ID agregado al historial:", currentId);  // Mensaje de depuración
    
    };
  </script>
